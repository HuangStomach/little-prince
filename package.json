    "element-ui": "^1.3.7",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "lru-cache": "^4.1.1",
    "serve-favicon": "^2.4.1",
    "vue": "^2.3.4",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.3.4",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-component": "^0.9.1",
